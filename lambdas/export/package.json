{
  "name": "moluh-export",
  "description": "Export payments",
  "version": "1.0.0",
  "main": "handler.js",
  "scripts": {
    "test:production": "set NODE_ENV=production && serverless offline",
    "build": "tsc && copyfiles -u 2 **/*.graphql -s dist/src/graphql",
    "build:dev": "node src/bin/www.ts --exec ts-node src/bin/www.ts",
    "clean": "rd /s/q dist",
    "start": "npm run clean && npm run build && node ./dist/src/bin/www.js",
    "start:dev": "ts-node src/bin/www.ts",
    "test": "jest",
    "test:watch": "jest --verbose --detectOpenHandles"
  },
  "homepage": "https://github.com/moluh/aws-serverless#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moluh/moluh-export.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moluh/moluh-export/issues"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.131.0",
    "@sendgrid/mail": "^7.4.6",
    "aws-sdk": "^2.989.0",
    "csv-parse": "^2.5.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^10.0.0",
    "easy-postgres": "^2.0.1",
    "event-stream": "^4.0.1",
    "is-number": "^7.0.0",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.33",
    "mongojs": "^3.1.0",
    "request": "^2.88.2",
    "serverless": "^3.19.0",
    "serverless-offline": "^8.8.0",
    "string-template-parser": "^1.2.6",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.127.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/express": "^4.17.13",
    "serverless-dotenv-plugin": "^4.0.1"
  }
}
